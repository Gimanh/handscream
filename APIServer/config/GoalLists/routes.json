[
    {
        "middlewares": [
            "isLoggedIn",
            "canAddComponent"
        ],
        "method": "POST",
        "route": "/module/goal_lists/add",
        "handler": "App\\Modules\\GoalLists\\Handlers\\AddGoalComponent"
    },
    {
        "middlewares": [
            "isLoggedIn",
            "canFetchGoalComponents"
        ],
        "method": "GET",
        "route": "/module/goal_lists",
        "handler": "App\\Modules\\GoalLists\\Handlers\\FetchAllGoalComponents"
    },
    {
        "middlewares": [
            "isLoggedIn",
            "canUpdateGoalComponent"
        ],
        "method": "POST",
        "route": "/module/goal_lists/update",
        "handler": "App\\Modules\\GoalLists\\Handlers\\UpdateGoalComponents"
    },
    {
        "middlewares": [
            "isLoggedIn",
            "canDeleteGoalComponent"
        ],
        "method": "POST",
        "route": "/module/goal_lists/delete",
        "handler": "App\\Modules\\GoalLists\\Handlers\\DeleteGoalComponent"
    }
]
