[
    {
        "middlewares": [
            "isLoggedIn"
        ],
        "method": "POST",
        "route": "/module/goalcomponents/add",
        "handler": "App\\Modules\\GoalComponents\\Handlers\\AddGoalComponent"
    },
    {
        "middlewares": [
            "isLoggedIn"
        ],
        "method": "GET",
        "route": "/module/goalcomponents/:goalId",
        "handler": "App\\Modules\\GoalComponents\\Handlers\\FetchAllGoalComponents"
    },
    {
        "middlewares": [
            "isLoggedIn"
        ],
        "method": "POST",
        "route": "/module/goalcomponents/update",
        "handler": "App\\Modules\\GoalComponents\\Handlers\\UpdateGoalComponents"
    },
    {
        "middlewares": [
            "isLoggedIn"
        ],
        "method": "POST",
        "route": "/module/goalcomponents/delete",
        "handler": "App\\Modules\\GoalComponents\\Handlers\\DeleteGoalList"
    }
]
